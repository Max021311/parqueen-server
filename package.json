    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-ts-standard": "^1.0.5",
    "sequelize-cli": "^6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.1",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "http-errors": "^2.0.0",
    "nodemon": "^2.0.22",
